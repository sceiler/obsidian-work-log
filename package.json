{
	"name": "work-log",
	"version": "1.0.0",
	"description": "Track daily work activities for performance reviews",
	"main": "main.js",
	"type": "module",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"lint": "eslint src/"
	},
	"keywords": [
		"obsidian",
		"plugin",
		"work-log",
		"performance-review"
	],
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"esbuild": "^0.25.0",
		"eslint": "^8.45.0",
		"tslib": "^2.4.0",
		"typescript": "^5.3.0"
	},
	"dependencies": {
		"obsidian": "latest"
	}
}
