{
	"name": "work-log",
	"version": "1.1.1",
	"description": "Track daily work activities for performance reviews",
	"main": "main.js",
	"type": "module",
	"engines": {
		"node": ">=24.0.0"
	},
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"test": "vitest",
		"test:run": "vitest run",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"lint": "eslint src/"
	},
	"keywords": [
		"obsidian",
		"plugin",
		"work-log",
		"performance-review"
	],
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^24.0.0",
		"esbuild": "^0.27.0",
		"eslint": "^9.39.0",
		"tslib": "^2.8.0",
		"typescript": "^5.8.0",
		"typescript-eslint": "^8.54.0",
		"vitest": "^4.0.18"
	},
	"dependencies": {
		"obsidian": "latest"
	}
}
